{
  "name": "pencil",
  "version": "1.0.0",
  "description": "jQuery view",
  "main": "views/view.coffee",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "devDependencies": {
    "browserify": "^5.11.1",
    "browserify-shim": "^3.7.0",
    "chai": "^1.9.1",
    "coffee-script": "^1.8.0",
    "coffeeify": "^0.7.0",
    "coveralls": "^2.11.1",
    "gulp": "^3.8.7",
    "gulp-bump": "^0.1.11",
    "gulp-git": "^0.5.0",
    "gulp-mocha": "^1.0.0",
    "gulp-mocha-phantomjs": "^0.5.0",
    "istanbul": "^0.3.2",
    "lodash": "^2.4.1",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "0.0.1",
    "mocha-phantomjs": "^3.5.0",
    "phantomjs": "^1.9.7-15",
    "poncho": "^0.1.1",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^1.0.2"
  },
  "browserify": {
    "transform": [
      "coffeeify"
    ]
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha-phantomjs test/runner/test.html",
    "istanbul": "coffee -c models test views && istanbul cover _mocha --report lcovonly -- -R spec && rm -rf ./coverage ./models/*.js ./models/**/*.js ./test/*.js ./test/**/*.js ./views/*.js ./views/**/*.js",
    "coveralls": "coffee -c models test views && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage ./models/*.js ./models/**/*.js ./test/*.js ./test/**/*.js ./views/*.js ./views/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trymore/pencil.git"
  },
  "keywords": [
    "jQuery",
    "view"
  ],
  "author": "Daisuke Mino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trymore/pencil/issues"
  },
  "homepage": "https://github.com/trymore/pencil"
}
