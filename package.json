{
  "name": "pencil",
  "version": "1.0.0",
  "description": "jQuery view",
  "main": "pencil.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "devDependencies": {
    "browserify": "^5.11.1",
    "browserify-shim": "^3.7.0",
    "chai": "^1.9.1",
    "coffee-script": "^1.8.0",
    "coffeeify": "^0.7.0",
    "coveralls": "^2.11.1",
    "gulp": "^3.8.7",
    "gulp-bump": "^0.1.11",
    "gulp-git": "^0.5.0",
    "gulp-mocha": "^1.0.0",
    "gulp-mocha-phantomjs": "^0.5.0",
    "jquery": "^2.1.1",
    "lodash": "^2.4.1",
    "mocha": "^1.21.4",
    "mocha-phantomjs": "^3.5.0",
    "phantomjs": "^1.9.7-15",
    "poncho": "^0.1.1",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^1.0.2"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim"
